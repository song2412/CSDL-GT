#include<stdio.h> 
#include<conio.h> 
struct DiemKG 
{ 
float x; 
float y; 
}; 
typedef struct DiemKG DIEMKG; 
void NhapDiemKG(DIEMKG &); 
void XuatDiemKG(DIEMKG); 
void NhapDiemKG(DIEMKG &d) 
{ 
float temp; 
printf("\nNhap x: "); 
scanf("%f", &temp); 
d.x = temp; 
printf("\nNhap y: "); 
scanf("%f", &temp);
d.y = temp; 
} 
void XuatDiemKG(DIEMKG d) 
{ 
printf("(%8.3f, %8.3f)", d.x, d.y); 
}
int main() 
{ 
DiemKG d; 
NhapDiemKG(d); 
XuatDiemKG(d); 
getch(); 
return 0; 
}
